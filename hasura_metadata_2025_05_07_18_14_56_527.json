    "query_collections": [
      {
        "name": "allowed-queries",
        "definition": {
          "queries": [
            {
              "name": "GetRoomsByHotel",
              "query": "query GetRoomsByHotel($hotel_id: Int!) {\r\n  rooms(where: {hotel_id: {_eq: $hotel_id}}) {\r\n    room_id\r\n    room_number\r\n    description\r\n    price\r\n    availability\r\n    status\r\n    room_type {\r\n      type_name\r\n    }\r\n  }\r\n}"
            },
            {
              "name": "GetAllLocations",
              "query": "query GetAllLocations {\r\n  locations {\r\n    location_id\r\n    location_name\r\n    description\r\n  }\r\n}"
            },
            {
              "name": "GetBookingsByRoom",
              "query": "query GetBookingsByRoom($room_id: Int!) {\r\n  bookings(where: {room_id: {_eq: $room_id}}) {\r\n    booking_id\r\n    guest_id\r\n    check_in_date\r\n    check_out_date\r\n    status\r\n    total_amount\r\n    payment_status\r\n  }\r\n}"
            },
            {
              "name": "GetRoomDetails",
              "query": "query GetRoomDetails($room_id: Int!) {\r\n  rooms_by_pk(room_id: $room_id) {\r\n    room_id\r\n    room_number\r\n    description\r\n    price\r\n    availability\r\n    status\r\n    room_type {\r\n      type_name\r\n      description\r\n    }\r\n    hotel {\r\n      hotel_name\r\n      location {\r\n        location_name\r\n      }\r\n    }\r\n    bookings {\r\n      check_in_date\r\n      check_out_date\r\n    }\r\n  }\r\n}"
            },
            {
              "name": "CreateGuest",
              "query": "mutation MyMutation($address1: String = \"\", $birthdate1: date = \"\", $contact_info1: jsonb = \"\", $full_name: String = \"\", $guest_id: Int!) {\r\n  insert_guests_one(object: {address: $address1, birthdate: $birthdate1, contact_info: $contact_info1, full_name: $full_name, guest_id: $guest_id}) {\r\n    address\r\n    birthdate\r\n    contact_info\r\n    full_name\r\n    guest_id\r\n  }\r\n}"
            },
            {
              "name": "GetAllBookings",
              "query": "query MyQuery {\r\n  bookings {\r\n    booking_id\r\n    room {\r\n      description\r\n      hotel {\r\n        hotel_name\r\n      }\r\n      price\r\n    }\r\n    check_in_date\r\n    check_out_date\r\n    guest_id\r\n    payment_status\r\n    total_amount\r\n  }\r\n}"
            },
            {
              "name": "GetGuestBookings",
              "query": "query GetGuestBookings($guest_id: Int!) {\r\n  bookings(where: { guest_id: { _eq: $guest_id } }) {\r\n    booking_id\r\n    room_id\r\n    check_in_date\r\n    check_out_date\r\n    status\r\n    total_amount\r\n    payment_status\r\n  }\r\n}"
            },
            {
              "name": "GetAllUsers",
              "query": "query MyQuery {\r\n  users(where: {role: {_eq: \"user\"}}) {\r\n    id\r\n    username\r\n    role\r\n    created_at\r\n  }\r\n}"
            },
            {
              "name": "GetGuestProfile",
              "query": "query GetGuestProfile($guest_id: Int!) {\r\n  guests_by_pk(guest_id: $guest_id) {\r\n    full_name\r\n    birthdate\r\n    contact_info\r\n    address\r\n  }\r\n}"
            },
            {
              "name": "GetGuestTransactions",
              "query": "query GetGuestTransactions($guest_id: Int!) {\r\n  payments(where: {guest_id: {_eq: $guest_id}}) {\r\n    payment_id\r\n    amount\r\n    payment_date\r\n    payment_status\r\n    payment_method\r\n  }\r\n}"
            },
            {
              "name": "GetGuestDetails",
              "query": "query GetGuestDetails($guest_id: Int!) {\r\n  guests_by_pk(guest_id: $guest_id) {\r\n    guest_id\r\n    full_name\r\n    address\r\n    birthdate\r\n    contact_info\r\n  }\r\n  bookings(where: { guest_id: { _eq: $guest_id } }) {\r\n    booking_id\r\n    check_in_date\r\n    check_out_date\r\n    status\r\n    total_amount\r\n  }\r\n  payments {\r\n    payment_id\r\n    booking_id\r\n    amount\r\n    payment_date\r\n    payment_status\r\n    payment_method\r\n  }\r\n}"
            },
            {
              "name": "GetAllHotels",
              "query": "query GetAllHotels {\r\n  hotels {\r\n    hotel_id\r\n    hotel_name\r\n    description\r\n    location {\r\n      location_id\r\n      location_name\r\n    }\r\n    rooms_aggregate {\r\n      aggregate {\r\n        min {\r\n          price\r\n        }\r\n      }\r\n    }\r\n  }\r\n}"
            },
            {
              "name": "CreateBooking",
              "query": "mutation CreateBooking(\r\n  $room_id: Int!\r\n  $guest_id: Int!\r\n  $check_in_date: date!\r\n  $check_out_date: date!\r\n  $total_amount: numeric!\r\n) {\r\n  insert_bookings_one(\r\n    object: {\r\n      room_id: $room_id\r\n      guest_id: $guest_id\r\n      check_in_date: $check_in_date\r\n      check_out_date: $check_out_date\r\n      total_amount: $total_amount\r\n      status: \"Pending\"\r\n      payment_status: \"Pending\"\r\n    }\r\n  ) {\r\n    booking_id\r\n  }\r\n}"
            },
            {
              "name": "CreatePayment",
              "query": "mutation CreatePayment(\n  $booking_id: Int!\n  $amount: numeric!\n  $payment_method: String!\n) {\n  insert_payments_one(object: {\n    booking_id: $booking_id,\n    amount: $amount,\n    payment_method: $payment_method,\n    payment_status: \"Pending\",\n    payment_date: \"now()\"\n  }) {\n    payment_id\n  }\n}"
            },
            {
              "name": "UpdateGuestProfile",
              "query": "mutation UpdateGuestProfile(\r\n  $guest_id: Int!\r\n  $full_name: String\r\n  $phone: String\r\n  $address: String\r\n  $birthdate: date\r\n) {\r\n  update_guests_by_pk(\r\n    pk_columns: { guest_id: $guest_id }\r\n    _set: {\r\n      full_name: $full_name\r\n      contact_info: { phone: $phone } # Update the entire contact_info object\r\n      address: $address\r\n      birthdate: $birthdate\r\n    }\r\n  ) {\r\n    guest_id\r\n    full_name\r\n    contact_info\r\n    address\r\n    birthdate\r\n  }\r\n}"
            },
            {
              "name": "UpdateHotelDetails",
              "query": "mutation UpdateHotelDetails(\r\n  $hotel_id: Int!,\r\n  $hotel_name: String!,\r\n  $description: String!,\r\n  $location_id: Int!,\r\n  $location_name: String!,\r\n  $location_description: String!\r\n) {\r\n  # Update the hotel details\r\n  update_hotels_by_pk(\r\n    pk_columns: { hotel_id: $hotel_id },\r\n    _set: {\r\n      hotel_name: $hotel_name,\r\n      description: $description\r\n    }\r\n  ) {\r\n    hotel_id\r\n    hotel_name\r\n    description\r\n  }\r\n\r\n  # Update the location details\r\n  update_locations_by_pk(\r\n    pk_columns: { location_id: $location_id },\r\n    _set: {\r\n      location_name: $location_name,\r\n      description: $location_description\r\n    }\r\n  ) {\r\n    location_id\r\n    location_name\r\n    description\r\n  }\r\n}"
            },
            {
              "name": "createHotel",
              "query": "mutation AddHotel(\r\n  $hotel_name: String!,\r\n  $location_name: String!,\r\n  $location_description: String!,\r\n  $description: String!\r\n) {\r\n  insert_locations_one(\r\n    object: {\r\n      location_name: $location_name,\r\n      description: $location_description,\r\n      hotels: {\r\n        data: {\r\n          hotel_name: $hotel_name,\r\n          description: $description\r\n        }\r\n      }\r\n    }\r\n  ) {\r\n    location_id\r\n    location_name\r\n    description\r\n    hotels {\r\n      hotel_id\r\n      hotel_name\r\n      description\r\n    }\r\n  }\r\n}"
            },
            {
              "name": "GetHotelData",
              "query": "query GetHotelData($hotel_id: Int!) {\r\n  # Fetch hotel details\r\n  hotels_by_pk(hotel_id: $hotel_id) {\r\n    hotel_id\r\n    hotel_name\r\n    description\r\n    location {\r\n      location_id\r\n      location_name\r\n      description\r\n    }\r\n    # Fetch rooms for the hotel\r\n    rooms {\r\n      room_id\r\n      room_number\r\n      description\r\n      price\r\n      availability\r\n      status\r\n      room_type {\r\n        type_name\r\n        description\r\n      }\r\n    }\r\n  }\r\n  # Fetch bookings for the hotel\r\n  bookings(where: { room: { hotel_id: { _eq: $hotel_id } } }) {\r\n    booking_id\r\n    guest_id\r\n    room_id\r\n    check_in_date\r\n    check_out_date\r\n    status\r\n    total_amount\r\n    payment_status\r\n    created_at\r\n    updated_at\r\n    room {\r\n      room_number\r\n      room_type {\r\n        type_name\r\n        description\r\n      }\r\n    }\r\n  }\r\n}"
            },
            {
              "name": "DeleteBooking",
              "query": "mutation DeleteBooking($booking_id: Int!) {\r\n  delete_bookings_by_pk(booking_id: $booking_id) {\r\n    booking_id\r\n  }\r\n}"
            },
            {
              "name": "AddRoom",
              "query": "mutation AddRoom(\r\n  $hotel_id: Int!,\r\n  $room_number: String!,\r\n  $type_id: Int!,\r\n  $description: String,\r\n  $price: numeric!,\r\n  $availability: Boolean!\r\n) {\r\n  insert_rooms_one(\r\n    object: {\r\n      hotel_id: $hotel_id,\r\n      room_number: $room_number,\r\n      type_id: $type_id,\r\n      description: $description,\r\n      price: $price,\r\n      availability: $availability\r\n    }\r\n  ) {\r\n    room_id\r\n  }\r\n}"
            },
            {
              "name": "EditRoom",
              "query": "mutation EditRoom(\r\n  $room_id: Int!,\r\n  $room_number: String!,\r\n  $type_id: Int!,\r\n  $description: String,\r\n  $price: numeric!,\r\n  $availability: Boolean!\r\n) {\r\n  update_rooms_by_pk(\r\n    pk_columns: { room_id: $room_id },\r\n    _set: {\r\n      room_number: $room_number,\r\n      type_id: $type_id,\r\n      description: $description,\r\n      price: $price,\r\n      availability: $availability\r\n    }\r\n  ) {\r\n    room_id\r\n    room_number\r\n    type_id\r\n    description\r\n    price\r\n    availability\r\n  }\r\n}"
            },
            {
              "name": "GetRoomTypesByHotel",
              "query": "query GetRoomTypesByHotel($hotel_id: Int!) {\r\n  room_types(where: { hotel_id: { _eq: $hotel_id } }) {\r\n    type_id\r\n    type_name\r\n    description\r\n  }\r\n}"
            },
            {
              "name": "UpdateRoomStatusAndAvailability",
              "query": "mutation UpdateRoomStatusAndAvailability($room_id: Int!, $status: String!, $availability: Boolean!) {\r\n  update_rooms_by_pk(\r\n    pk_columns: { room_id: $room_id },\r\n    _set: { status: $status, availability: $availability }\r\n  ) {\r\n    room_id\r\n    status\r\n    availability\r\n  }\r\n}"
            },
            {
              "name": "GetRoomsByType",
              "query": "query GetRoomsByType($type_id: Int!) {\r\n  rooms(where: { type_id: { _eq: $type_id } }) {\r\n    room_id\r\n    room_number\r\n    price\r\n    availability\r\n    status\r\n  }\r\n}"
            },
            {
              "name": "UpdateRoomType",
              "query": "mutation UpdateRoomType($type_id: Int!, $type_name: String!, $description: String!) {\r\n  update_room_types_by_pk(\r\n    pk_columns: { type_id: $type_id },\r\n    _set: { type_name: $type_name, description: $description }\r\n  ) {\r\n    type_id\r\n    type_name\r\n    description\r\n  }\r\n}"
            },
            {
              "name": "GetRoomTypeById",
              "query": "query GetRoomTypeById($type_id: Int!) {\r\n  room_types_by_pk(type_id: $type_id) {\r\n    type_id\r\n    type_name\r\n    description\r\n  }\r\n}"
            },
            {
              "name": "DeleteRoomType",
              "query": "mutation DeleteRoomType($type_id: Int!) {\r\n  delete_room_types_by_pk(type_id: $type_id) {\r\n    type_id\r\n    type_name\r\n  }\r\n}"
            },
            {
              "name": "AddRoomType",
              "query": "mutation AddRoomType($type_name: String!, $description: String!, $hotel_id: Int!) {\r\n  insert_room_types_one(\r\n    object: { type_name: $type_name, description: $description, hotel_id: $hotel_id },\r\n    on_conflict: {\r\n      constraint: room_types_type_name_key,  \r\n      update_columns: [description]\r\n    }\r\n  ) {\r\n    type_id\r\n    type_name\r\n    description\r\n    hotel_id\r\n  }\r\n}"
            }
          ]
        }
